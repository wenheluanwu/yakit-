ret = codec.EncodeHtml("abcdefghijklmn\xA0")
println("Html编码为：",ret)
println("-"*15)
res = codec.DecodeHtml(ret)
println("解码后为：")
dump([]byte(res))
println("-"*15)
ret = codec.EncodeHtmlHex("abcdefghijklmn\xA0")
println("Html Hex编码为：",ret)
res = codec.DecodeHtml(ret)
println("解码后为：")
dump([]byte(res))
println("-"*15)
ret = codec.EscapeHtml("abcdefghij<>:;{]{]|\\$^&^&#@klmn\xA0")
println("Escape编码为：",ret)
res = codec.DecodeHtml(ret)
println("解码后为: ")
dump([]byte(res))